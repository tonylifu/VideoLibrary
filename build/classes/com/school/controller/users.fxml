<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.school.controller.UserLoginController">
   <center>
      <GridPane prefHeight="539.0" prefWidth="570.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="279.0" minWidth="10.0" prefWidth="138.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="432.0" minWidth="10.0" prefWidth="432.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="66.0" prefWidth="135.0" text="Password" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" prefHeight="66.0" prefWidth="135.0" text="Name" GridPane.rowIndex="2">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" prefHeight="66.0" prefWidth="135.0" text="User Class" GridPane.rowIndex="3">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" prefHeight="66.0" prefWidth="135.0" text="Department" GridPane.rowIndex="4">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" prefHeight="66.0" prefWidth="135.0" text="User Name">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <PasswordField fx:id="password_TextField" prefHeight="58.0" prefWidth="432.0" promptText="password" style="-fx-background-color: skyblue;" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font name="Times New Roman" size="18.0" />
               </font>
            </PasswordField>
            <TextField fx:id="name_TextField" prefHeight="60.0" prefWidth="432.0" style="-fx-background-color: skyblue;" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font name="Times New Roman" size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="userName_TextField" prefHeight="60.0" prefWidth="397.0" style="-fx-background-color: skyblue;" GridPane.columnIndex="1">
               <font>
                  <Font name="Times New Roman" size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="userClass_TextField" prefHeight="60.0" prefWidth="432.0" style="-fx-background-color: skyblue;" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <font>
                  <Font name="Times New Roman" size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="dept_TextField" prefHeight="60.0" prefWidth="432.0" style="-fx-background-color: skyblue;" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <font>
                  <Font name="Times New Roman" size="18.0" />
               </font>
            </TextField>
            <ToolBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
              <items>
                  <Button mnemonicParsing="false" onAction="#verifyuserNameButton" prefHeight="44.0" prefWidth="95.0" text="VerifyUser" textFill="#401616">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#deleteUserNameButton" prefHeight="44.0" prefWidth="95.0" text="Delete" textFill="#401616">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#resetUserButton" prefHeight="44.0" prefWidth="95.0" text="Reset" textFill="#401616">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#createUserButton" prefHeight="45.0" prefWidth="121.0" text="Save" textFill="#401616">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
              </items>
            </ToolBar>
          <Button fx:id="logoutAdminButton" mnemonicParsing="false" onAction="#handleButtonAction6" prefHeight="44.0" prefWidth="95.0" text="Logout" textFill="#401616" GridPane.rowIndex="5">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
            </Button>
         </children>
      </GridPane>
   </center>
</BorderPane>
