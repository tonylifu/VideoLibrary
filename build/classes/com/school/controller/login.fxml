<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.school.controller.UserLoginController">
   <top>
      <GridPane prefHeight="455.0" prefWidth="505.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="244.0" minWidth="10.0" prefWidth="81.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="432.0" minWidth="10.0" prefWidth="424.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="147.0" minHeight="10.0" prefHeight="90.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="147.0" minHeight="10.0" prefHeight="55.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="233.0" minHeight="10.0" prefHeight="66.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="343.0" minHeight="10.0" prefHeight="230.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="60.0" prefWidth="124.0" text="Password" GridPane.rowIndex="2">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" prefHeight="60.0" prefWidth="124.0" text="User Name" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordLogin_TF" prefHeight="47.0" prefWidth="382.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <opaqueInsets>
                  <Insets bottom="5.0" left="5.0" right="20.0" top="5.0" />
               </opaqueInsets></PasswordField>
            <TextField fx:id="userNameLogin_TF" prefHeight="43.0" prefWidth="382.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <opaqueInsets>
                  <Insets bottom="5.0" left="5.0" right="20.0" top="5.0" />
               </opaqueInsets></TextField>
            <ToolBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
              <items>
                <Button mnemonicParsing="false" onAction="#quitPlatformButton2" prefHeight="41.0" prefWidth="108.0" text="Quit" textFill="#153618">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="adminButton" mnemonicParsing="false" onAction="#handleButtonAction6" prefHeight="41.0" prefWidth="108.0" text="Login Admin" textFill="#153618">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#loginButton" prefHeight="41.0" prefWidth="108.0" text="Login" textFill="#153618">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
              </items>
            </ToolBar>
         </children>
      </GridPane>
   </top>
</BorderPane>
